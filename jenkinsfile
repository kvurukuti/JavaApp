pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('COPY') {
            steps {
                sh 'cp /var/lib/jenkins/workspace/login pipeline/target/login.war /opt/jenkins/'
            }
        }
        stage('Complete') {
            steps {
                echo "job is completed"
            }
        }
    }
}
